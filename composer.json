{
    "name": "prinsfrank/glyph-lists",
    "description": "Pre-parsed glyph lists as enums",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PrinsFrank\\GlyphLists\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PrinsFrank\\GlyphLists\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.87",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpunit/phpunit": "^12.3"
    },
    "scripts": {
        "cs": "php-cs-fixer fix --allow-risky=yes --dry-run",
        "cs:fix": "php-cs-fixer fix --allow-risky=yes",
        "stan": "phpstan analyse --memory-limit 4G -v",
        "unit": "phpunit --testsuite=Unit"
    },
    "config": {
        "sort-packages": true
    }
}
